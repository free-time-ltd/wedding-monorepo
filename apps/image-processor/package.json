    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156",
    "@types/node": "^24.8.1",
    "eslint": "^9.38.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  },
  "turbo": {
    "env": [
      "WEBHOOK_URL",
      "WEBHOOK_SECRET",
      "BUCKET_NAME",
      "AWS_REGION"
